<h1>Users#index</h1>

<%= render 'books/user', user: @user, book: @book %>

<h1>Users</h1>

<% @users.each do |user| %>
  <table>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
    <tr>
      <td>
        <%= image_tag user.get_profile_image(100,100) %>
      </td>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= link_to "Show", user_path(user.id) %>
      </td>
    </tr>
  </table>
<% end %>